mdl 1.3;

using state import *;

import adobe::mtl::*;
import adobe::util::*;
import ::tex::gamma_mode;
import ::state::normal;

export material WoodTableUp_Mat(*)
  = adobe::mtl::standard(
    opacity : adobe::util::float_texture( texture_2d("VintageBasin/WoodTableUp_opacity.png", ::tex::gamma_linear) ),
    baseColor : adobe::util::color_texture( texture_2d("VintageBasin/WoodTableUp_baseColor.png", ::tex::gamma_srgb) ),
    translucence : float(0.0),
    glow : adobe::util::float_texture( texture_2d("VintageBasin/WoodTableUp_glow.png", ::tex::gamma_linear) ),
    indexOfRefraction : float(1.45),
    density : float(0.0),
    roughness : adobe::util::float_texture( texture_2d("VintageBasin/WoodTableUp_roughness.png", ::tex::gamma_linear) ),
    normal : adobe::util::normal_texture( texture_2d("VintageBasin/WoodTableUp_normal.png", ::tex::gamma_linear) ),
    interiorColor : color(1.0, 1.0, 1.0),
    metallic : adobe::util::float_texture( texture_2d("VintageBasin/WoodTableUp_metallic.png", ::tex::gamma_linear) ),
    height : float(0.5),
    heightScale : float(1.0)
    );

